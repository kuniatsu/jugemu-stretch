  { "id": "quad_standing", "title": "大腿四頭筋の立位ストレッチ", "description": "片足を後ろに曲げ、同じ側の手で足首を持ち、太もも前面を伸ばします。", "target_muscle_ids": ["quadriceps"], "duration_seconds": 30, "is_sided": true, "image_resource_name": "stretch_quad_stand" },
  { "id": "seiza_backbend", "title": "正座後屈", "description": "正座の状態からゆっくり後ろに倒れ、太もも前面と股関節を伸ばします。無理のない範囲で。", "target_muscle_ids": ["quadriceps", "hip_flexor"], "duration_seconds": 30, "is_sided": false, "image_resource_name": "stretch_seiza_backbend" },
  { "id": "shin_stretch", "title": "前脛骨筋のストレッチ", "description": "正座の姿勢からつま先を伸ばし、すね前面を伸ばします。", "target_muscle_ids": ["tibialis_anterior"], "duration_seconds": 30, "is_sided": false, "image_resource_name": "stretch_shin" },
