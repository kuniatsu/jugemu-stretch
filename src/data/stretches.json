  { "id": "seated_twist", "title": "座位のツイストストレッチ", "description": "座って片足を反対側の膝の外に置き、体をひねります。", "target_muscle_ids": ["erector_spinae", "obliques"], "duration_seconds": 30, "is_sided": true, "image_resource_name": "stretch_seated_twist" },
