  { "id": "sleeper_stretch", "title": "スリーパーストレッチ", "description": "横向きに寝て、下の腕を前に伸ばし、もう片方の手で手首を床に向かって押します。", "target_muscle_ids": ["rotator_cuff"], "duration_seconds": 30, "is_sided": true, "image_resource_name": "stretch_sleeper" }
