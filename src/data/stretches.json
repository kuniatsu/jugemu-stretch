  { "id": "upper_back_clasp", "title": "背中の手組みストレッチ", "description": "手を前で組んで背中を丸め、肩甲骨の間を開きます。", "target_muscle_ids": ["rhomboids", "trapezius_mid"], "duration_seconds": 30, "is_sided": false, "image_resource_name": "stretch_upper_back" },
