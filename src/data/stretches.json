  { "id": "neck_backward", "title": "首の後倒し", "description": "ゆっくりと頭を後ろに倒し、首の前面を伸ばします。無理のない範囲で行ってください。", "target_muscle_ids": ["sternocleidomastoid"], "duration_seconds": 30, "is_sided": false, "image_resource_name": "stretch_neck_backward" },
  { "id": "shoulder_cross_body", "title": "肩のクロスボディストレッチ", "description": "片腕を胸の前で反対側に伸ばし、もう片方の手で押さえます。", "target_muscle_ids": ["posterior_deltoid", "rhomboids"], "duration_seconds": 30, "is_sided": true, "image_resource_name": "stretch_shoulder_cross" },
