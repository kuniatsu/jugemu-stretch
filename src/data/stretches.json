[
  { "id": "neck_side_tilt", "title": "首の横倒し", "description": "頭をゆっくり横に倒し、反対側の首筋を伸ばします。手で軽く押さえるとより効果的です。", "target_muscle_ids": ["sternocleidomastoid", "trapezius_upper"], "duration_seconds": 30, "is_sided": true, "image_resource_name": "stretch_neck_side" },
